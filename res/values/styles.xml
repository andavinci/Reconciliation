<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <!-- Login Xml UserName PassWord Style -->
    <style name="LoginUserNamePwdTheme">
        <item name="android:layout_width">@dimen/login_width</item>
        <item name="android:layout_height">@dimen/login_height</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColorHint">@color/font_color2</item>
        <item name="android:paddingLeft">@dimen/gv_line_horizontal_width</item>
        <item name="android:drawablePadding">@dimen/gv_line_horizontal_width</item>
    </style>
    <!-- Login Xml UserName PassWord Style -->
    <style name="LoginRegisterTheme">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">@drawable/text_select</item>
    </style>

    <!-- title_text -->
    <style name="activity_title_text">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:text">@string/category_communication</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
        <item name="android:focusable">true</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:paddingLeft">@dimen/title_padding_size</item>
        <item name="android:paddingRight">@dimen/title_padding_size</item>
    </style>
    <!-- title_text -->
    <style name="activity_title_right_text">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:text">@string/category_communication</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
        <item name="android:focusable">true</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:paddingLeft">@dimen/title_padding_size</item>
        <item name="android:paddingRight">@dimen/title_padding_size</item>
    </style>
    <!-- title_text -->
    <style name="above_name_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:gravity">center</item>
        <item name="android:text">@string/category_communication</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
        <item name="android:focusable">true</item>
        <item name="android:scrollHorizontally">true</item>
    </style>
    <!-- Heading Style -->
    <style name="AllTitleTopTheme">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">56dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/title_text_size</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
        <item name="android:focusable">true</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:paddingLeft">@dimen/title_padding_size</item>
        <item name="android:paddingRight">@dimen/title_padding_size</item>
    </style>

    <style name="layout_title_text" parent="activity_title_text">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/title_gb_height</item>
        <item name="android:text">@string/category_communication</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@color/title_bg_color</item>
    </style>
    <!-- tabhost -->
    <style name="main_tab_but_linear">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:clickable">true</item>
    </style>

